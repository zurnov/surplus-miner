{
  "env": { "node": true, "es2021": true },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["import", "node", "prettier"],
  "parserOptions": { "ecmaVersion": 12, "sourceType": "module" },
  "rules": {
    "no-console": "warn",
    "no-underscore-dangle": "off",
    "prettier/prettier": ["error"]
  }
}